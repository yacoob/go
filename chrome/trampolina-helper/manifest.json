{
  "name": "Trampolina helper",
  "version": "0.4",
  "manifest_version": 2,

  "description": "Helper for trampolina",
  "browser_action": {
    "default_icon": "res/icon48.png",
    "default_title": "Sproing!",
    "default_popup": "popup.html"
  },
  "background": {
    "page": "background.html"
  },
  "options_page": "options.html",
  "permissions": [
    "background",
    "storage",
    "tabs",
    "http://*/",
    "https://*/"
  ],
  "icons": {
    "128": "res/icon128.png",
    "48":  "res/icon48.png"
  }
}
